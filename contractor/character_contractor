public class character {
        // Character attributes
    int strength;
    int agility;
    int intelligence;
    String name;

    // Character method
    public void sayMyName(String characterName) {
        System.out.println("Hello I am " + characterName);
    }

    // Constructor
    public character(int str, int agi, int intel, String characterName) {
        strength = str;
        agility = agi;
        intelligence = intel;
        name = characterName;
        System.out.println("My name is: " + characterName + "\n" + " My strenght is: " + str + "\n" + " My agility is: " + agi + "\n" + " My intelligence: " + intel);
    }
}

